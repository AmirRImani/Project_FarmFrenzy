<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="750.0" prefWidth="1000.0" style="-fx-background-color: #bd994c;" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.WarehouseView">
   <children>
      <AnchorPane fx:id="anchorPane2" layoutX="325.0" prefHeight="750.0" prefWidth="300.0" styleClass="list-anchorPane" stylesheets="@styles/backgrounds/backgroundWarehouse.css" />
      <AnchorPane fx:id="anchorPane1" prefHeight="750.0" prefWidth="300.0" styleClass="list-anchorPane" stylesheets="@styles/backgrounds/backgroundWarehouse.css" />
      <ImageView fx:id="imgTruck" fitHeight="250.0" fitWidth="360.0" layoutX="625.0" layoutY="248.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/objects/Truck.png" />
         </image>
      </ImageView>
      <Button layoutX="730.0" layoutY="605.0" mnemonicParsing="false" onAction="#back" prefHeight="120.0" prefWidth="217.0" styleClass="continue-button" stylesheets="@styles/backgrounds/backgroundWarehouse.css" />
      <Button layoutX="655.0" layoutY="56.0" mnemonicParsing="false" onAction="#truckLoad" prefHeight="90.0" prefWidth="150.0" stylesheets="@styles/buttonStyles/buttonPressedYellow.css" text="Truck Load" textFill="BLUE">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button layoutX="816.0" layoutY="56.0" mnemonicParsing="false" onAction="#truckUnload" prefHeight="90.0" prefWidth="150.0" stylesheets="@styles/buttonStyles/buttonPressedYellow.css" text="Truck Unload" textFill="BLUE">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button layoutX="730.0" layoutY="465.0" mnemonicParsing="false" onAction="#truckGo" prefHeight="120.0" prefWidth="217.0" styleClass="truckGo-button" stylesheets="@styles/backgrounds/backgroundWarehouse.css" />
   </children>
</AnchorPane>
