<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="600.0" prefWidth="800.0" styleClass="bodybg" stylesheets="@styles/backgrounds/backgroundLevelChooser.css" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.LevelChooser">
   <children>
      <Button fx:id="btnLevel1" layoutX="67.0" layoutY="352.0" mnemonicParsing="false" onAction="#level1" prefHeight="74.0" prefWidth="97.0" styleClass="first_button" stylesheets="@styles/backgrounds/backgroundLevelButton.css" textFill="WHITE" />
      <Button alignment="CENTER" layoutX="136.0" layoutY="499.0" mnemonicParsing="false" onAction="#exit" opacity="0.8" prefHeight="60.0" prefWidth="150.0" stylesheets="@styles/buttonStyles/buttonPressedRed.css" text="Exit" textFill="BLUE">
         <font>
            <Font size="28.0" />
         </font>
      </Button>
      <Button alignment="CENTER" layoutX="552.0" layoutY="499.0" mnemonicParsing="false" onAction="#back" opacity="0.8" prefHeight="60.0" prefWidth="150.0" stylesheets="@styles/buttonStyles/buttonPressedYellow.css" text="Logout" textFill="RED">
         <font>
            <Font size="24.0" />
         </font>
      </Button>
      <ImageView fx:id="imgLevel1" fitHeight="60.0" fitWidth="65.0" layoutX="85.0" layoutY="360.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/circle-cropped.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
